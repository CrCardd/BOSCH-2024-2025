<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="box" prefHeight="148.0" prefWidth="344.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.desktopapp.LoginSceneController">
  <children>
    <MenuBar prefHeight="0.0" prefWidth="640.0" VBox.vgrow="NEVER" />
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="165.0" prefWidth="332.0" VBox.vgrow="ALWAYS">
         <children>
            <Button id="btLogin" fx:id="btLogin" layoutX="269.0" layoutY="97.0" mnemonicParsing="false" onAction="#submit" text="Entrar" />
            <Label layoutX="28.0" layoutY="31.0" text="login" />
            <Label layoutX="25.0" layoutY="64.0" text="Senha" />
            <TextField id="tfLogin" fx:id="tfLogin" layoutX="93.0" layoutY="27.0" prefHeight="25.0" prefWidth="223.0" promptText="email ou nome de usuário" />
            <PasswordField id="pfPass" fx:id="pfPass" layoutX="93.0" layoutY="60.0" prefHeight="25.0" prefWidth="223.0" />
            <Label layoutX="25.0" layoutY="110.0" text="Não tem uma conta?" />
            <Label layoutX="146.0" layoutY="110.0" onMouseClicked="#CreateAccount" text="Criar uma" textFill="#5f43ff" />
         </children>
    </AnchorPane>
  </children>
</VBox>
